<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
            prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.281" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.EntranceController">
    <VBox alignment="CENTER" layoutX="280" layoutY="200" prefHeight="200.0" prefWidth="240.0">
        <Button fx:id="defaultButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#defaultButtonTapped"
                prefHeight="25.0" prefWidth="230.0" text="Connect as Khlimankova">
            <VBox.margin>
                <Insets bottom="15.0"/>
            </VBox.margin>
        </Button>

        <Button fx:id="localhostButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#localhostButtonTapped"
                prefHeight="25.0" prefWidth="230.0" text="Connect to localhost"/>
    </VBox>
</AnchorPane>

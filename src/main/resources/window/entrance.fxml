<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.281" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.LoginController">

    <VBox alignment="CENTER" layoutX="50.0" layoutY="35" prefHeight="300.0" prefWidth="260.0" spacing="20">
        <Button fx:id="defaultButton" AnchorPane.leftAnchor="100" contentDisplay="CENTER" mnemonicParsing="false" onAction="#defaultButtonTapped"
          prefHeight="40.0" prefWidth="190.0" text="Connect to NSU">
        </Button>

        <Button fx:id="localhostButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#localhostButtonTapped"
          prefHeight="40.0" prefWidth="190.0" text="Connect to localhost"/>
    </VBox>

</AnchorPane>
